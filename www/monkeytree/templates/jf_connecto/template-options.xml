				<option value="off">No</option>
				<option value="on">Yes</option>
			</field>
		</fields>
		<fields name="jf_animate" 				type="chain" label="JF - CSS3 Animate">
			<field name="enabled" 				type="toggle" 			default="1" label="Enable/Disable" />
			
			<field name="jf_animate_all" 			type="toggle" 	label="All Animations" 				default="0" description="Enable All CSS3 available Animations (It loads heavy stylesheets)"/>
			<field name="jf_animate_all_sheet" 		type="text" 	label="CSS3 Animate Stylesheet" 	default="//cdnjs.cloudflare.com/ajax/libs/animate.css/3.2.0/animate.min.css" description="Type URl of CSS3 animate Stylesheet. It will be applied when 'All Animations' option is enabled above" class="text-long"/>
			<field name="jf_animate_custom" 		type="toggle" 	label="Custom Animations" 			default="1" description="It will load your 'Custom Animation' Styles (you have to manually edit 'custom_animate.min.css' file)"/>
			
			<field name="jf_animate_1" 				type="toggle" 	label="1-st Animation" 				default="0" description="Enable/Disable"/>
			<field name="jf_animate_1_type" 		type="text" 	label="1) Type Animation Type" 		default="bounce" description="Type CSS3 Animation - find Animation Lists on - http://daneden.github.io/animate.css/" class="text-medium"/>
